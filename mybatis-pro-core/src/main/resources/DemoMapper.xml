<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.dreamroute.fast.mapper.sdk.DemoMapper">
	<insert id="insert">
		insert into demo (`name`, password, version) values (#{name}, #{password}, #{version})
	</insert>
	<select id="findById" parameterType="long" resultType="com.github.dreamroute.fast.mapper.sdk.Demo">
		select * from demo where id = #{id}
	</select>
</mapper>