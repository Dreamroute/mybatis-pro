<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.dreamroute.mybatis.pro.core.interceptor.LogicalDeleteMapper">
    <insert id="backup" parameterType="java.util.List">
        insert into logical_delete (table_name, data) values
        <foreach collection="list" item="item" index="index" separator=",">
            #{item.tableName}, #{item.data}
        </foreach>
    </insert>
</mapper>